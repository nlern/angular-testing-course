{
  "name": "angular-testing-course",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --proxy-config ./proxy.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "prep:coverage": "jest --no-watch",
    "view:coverage": "http-server -c-1 -o coverage/",
    "coverage": "run-s prep:coverage view:coverage",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --configuration production",
    "start:prod": "http-server ./dist -a localhost -p 4200",
    "build-and-start:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build-and-start:prod http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "12.0.1",
    "@angular/compiler": "12.0.1",
    "@angular/core": "12.0.1",
    "@angular/forms": "12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/material-moment-adapter": "12.0.1",
    "@angular/platform-browser": "12.0.1",
    "@angular/platform-browser-dynamic": "12.0.1",
    "@angular/router": "12.0.1",
    "body-parser": "^1.18.3",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "http-server": "^0.12.3",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.5",
    "rxjs": "6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.2",
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "12.0.1",
    "@angular/language-service": "12.0.1",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@types/express": "^4.0.39",
    "@types/jest": "27.0.2",
    "@types/node": "^12.11.1",
    "cypress": "^8.5.0",
    "jest": "27.2.4",
    "jest-preset-angular": "^10.0.1",
    "start-server-and-test": "^1.9.1",
    "ts-node": "~3.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.2.4"
  }
}
